{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "pnpm -C \"./../../\" exec astro dev",
    "build": "nx exec -- pnpm run _build ",
    "preview": "pnpm -C \"./../../\" exec astro preview",
    "lint": "nx exec -- pnpm -C \"./../../\" exec astro check",
